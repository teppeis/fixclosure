{
  "name": "fixclosure",
  "description": "JavaScript dependency checker/fixer for Closure Library based on ECMAScript AST",
  "version": "4.0.1",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=18"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "fixclosure": "./bin/fixclosure.js"
  },
  "files": [
    "bin",
    "lib",
    "types"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib .tsbuildinfo",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "test": "run-s clean build lint:* unit",
    "unit": "mocha"
  },
  "dependencies": {
    "@teppeis/doctrine": "^3.2.0",
    "@types/cli-color": "^2.0.6",
    "@types/estraverse": "5.1.7",
    "@types/estree": "1.0.5",
    "@types/estree-jsx": "^1.0.5",
    "cli-color": "^2.0.4",
    "commander": "^11.1.0",
    "espree": "^9.6.1",
    "estraverse-fb": "^1.3.2",
    "globby": "^14.0.1",
    "google-closure-deps": ">=20190325.0.0",
    "lodash.difference": "^4.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/lodash.difference": "^4.5.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.31",
    "@types/sinon": "^17.0.3",
    "chai": "^4.4.1",
    "eslint": "^8.57.0",
    "eslint-config-teppeis": "^19.1.2",
    "mocha": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sinon": "^17.0.1",
    "tempy": "^3.1.0",
    "ts-node": "^10.9.2",
    "ts-node-test-register": "^10.0.0",
    "typescript": "^5.4.4"
  },
  "repository": "https://github.com/teppeis/fixclosure",
  "license": "MIT"
}
