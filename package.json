{
  "name": "fixclosure",
  "description": "JavaScript dependency checker/fixer for Closure Library based on ECMAScript AST",
  "version": "2.5.0",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=10"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "fixclosure": "./bin/fixclosure.js"
  },
  "files": [
    "bin",
    "lib",
    "types"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib .tsbuildinfo .nyc_output coverage",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ext js,ts src types test",
    "test": "run-s clean lint build unit:coverage",
    "unit": "mocha",
    "unit:coverage": "nyc --reporter=html --reporter=text mocha"
  },
  "dependencies": {
    "@teppeis/doctrine": "^3.2.0",
    "@types/cli-color": "^0.3.30",
    "@types/estree": "0.0.42",
    "array.prototype.flat": "^1.2.3",
    "cli-color": "^2.0.0",
    "commander": "^4.1.1",
    "espree": "^6.1.2",
    "estraverse": "^4.3.0",
    "estraverse-fb": "^1.3.2",
    "globby": "^11.0.0",
    "google-closure-deps": ">=20190325.0.0",
    "lodash.difference": "^4.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/estraverse": "0.0.6",
    "@types/lodash.difference": "^4.5.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.13",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-teppeis": "^11.0.0",
    "mocha": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^8.1.1",
    "tempy": "^0.3.0",
    "ts-node": "^8.5.4",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.7.5"
  },
  "repository": "https://github.com/teppeis/fixclosure",
  "license": "MIT"
}
