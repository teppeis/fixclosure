{
  "name": "fixclosure",
  "description": "JavaScript dependency checker/fixer for Closure Library based on ECMAScript AST",
  "version": "2.4.2",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=10"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "fixclosure": "./bin/fixclosure.js"
  },
  "files": [
    "bin",
    "lib",
    "types"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib .tsbuildinfo .nyc_output coverage",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ext js,ts src types test",
    "test": "run-s clean lint build unit:coverage",
    "unit": "mocha",
    "unit:coverage": "nyc --reporter=html --reporter=text mocha"
  },
  "dependencies": {
    "@teppeis/doctrine": "^3.2.0",
    "@types/cli-color": "^0.3.29",
    "@types/estree": "0.0.39",
    "array.prototype.flat": "^1.2.1",
    "cli-color": "^1.4.0",
    "commander": "^3.0.1",
    "espree": "^6.0.0",
    "estraverse": "^4.3.0",
    "estraverse-fb": "^1.3.2",
    "google-closure-deps": ">=20190325.0.0",
    "lodash.difference": "^4.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/estraverse": "0.0.6",
    "@types/lodash.difference": "^4.5.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.19",
    "@types/sinon": "^7.0.13",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.1.0",
    "eslint-config-teppeis": "^10.1.2",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0",
    "tempy": "^0.3.0",
    "ts-node": "^8.4.1",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.6.3"
  },
  "repository": "https://github.com/teppeis/fixclosure",
  "license": "MIT"
}
