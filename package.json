{
  "name": "fixclosure",
  "description": "JavaScript dependency checker/fixer for Closure Library based on ECMAScript AST",
  "version": "2.2.0",
  "author": "Teppei Sato <teppeis@gmail.com>",
  "engines": {
    "node": ">=10"
  },
  "main": "index.js",
  "bin": {
    "fixclosure": "./bin/fixclosure.js"
  },
  "files": [
    "index.js",
    "bin",
    "lib"
  ],
  "scripts": {
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint *.js lib test",
    "test": "npm run lint && npm run unit:coverage",
    "unit": "mocha",
    "unit:coverage": "nyc --reporter=html --reporter=text mocha"
  },
  "dependencies": {
    "@babel/parser": "^7.4.5",
    "array.prototype.flat": "^1.2.1",
    "cli-color": "^1.4.0",
    "commander": "^2.20.0",
    "doctrine": "^3.0.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.2",
    "google-closure-deps": ">=20190325.0.0",
    "lodash.difference": "^4.5.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-teppeis": "^9.6.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "tempy": "^0.3.0"
  },
  "repository": "https://github.com/teppeis/fixclosure",
